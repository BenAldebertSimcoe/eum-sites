<settings>
  <common>
    <sharePointOnline>true</sharePointOnline>
    <useMixedModeAuthentication>false</useMixedModeAuthentication>
    <siteLists>
      <siteListName>Sites</siteListName>
    </siteLists>
    <defaultEnvironment></defaultEnvironment>
    <managedPaths>
      <path>/sites</path>
      <path>/teams</path>
    </managedPaths>
  </common>
  <environments>
    <environment>
      <id>1</id>
      <name>Envision IT Dev</name>
      <webApp>
        <url>https://envisionitdev.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>envisionitdev.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
      </webApp>
    </environment>
    <environment>
      <id>2</id>
      <name>Envision IT Dev Azure Key Vault</name>
      <webApp>
        <url>https://envisionitdev.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>envisionitdev.sharepoint.com Azure Key Vault</managedCredentials>
        <managedCredentialsType>AzureKeyVault</managedCredentialsType>
        <tenantID>64a54bf1-8d0d-4741-a2ae-36eb0bfa4c58</tenantID>
        <vaultName>EUMSites</vaultName>
      </webApp>
    </environment>
    <environment>
      <id>3</id>
      <name>EUM Demo</name>
      <webApp>
        <url>https://eumdemo.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>eumdemo.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
      </webApp>
      <EUM>
        <managedEUMCredentials>login.eumdemo.com</managedEUMCredentials>
        <SystemConfiguration_FK>1</SystemConfiguration_FK>
        <Domain_FK>1</Domain_FK>
        <EUMURL>https://login.eumdemo.com</EUMURL>
      </EUM>
    </environment>
    <environment>
      <id>4</id>
      <name>EUM Demo Azure Key Vault</name>
      <webApp>
        <url>https://eumdemo.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing2</sitesListSiteCollectionPath>
        <managedCredentials>eumdemo.sharepoint.com Azure Key Vault</managedCredentials>
        <managedCredentialsType>AzureKeyVault</managedCredentialsType>
        <tenantID>84cd9e31-3fbf-467a-8c40-61b240c7bcb2</tenantID>
        <vaultName>EUMDemo-EUMSites</vaultName>
      </webApp>
    </environment>
    <environment>
      <id>5</id>
      <name>Envision IT</name>
      <webApp>
        <url>https://envisionit.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>envisionit.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
      </webApp>
    </environment>
    <environment>
      <id>6</id>
      <name>EIT Valo</name>
      <webApp>
        <url>https://eitvalo.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/teamwork-modern</sitesListSiteCollectionPath>
        <managedCredentials>eitvalo.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
      </webApp>
      <EUM>
        <managedEUMCredentials>login.eumdemo.com</managedEUMCredentials>
        <SystemConfiguration_FK>1</SystemConfiguration_FK>
        <Domain_FK>1</Domain_FK>
        <EUMURL>https://login.eumdemo.com</EUMURL>
      </EUM>
    </environment>
  </environments>
</settings>
