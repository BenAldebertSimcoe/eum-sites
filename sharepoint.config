<settings>
  <common>
    <sharePointOnline>true</sharePointOnline>
    <useMixedModeAuthentication>false</useMixedModeAuthentication>
    <siteLists>
      <siteListName>Sites</siteListName>
    </siteLists>
    <defaultEnvironment></defaultEnvironment>
    <managedPaths>
      <path>/sites</path>
      <path>/teams</path>
    </managedPaths>
  </common>
  <environments>
    <environment>
      <id>1</id>
      <name>Envision IT Dev</name>
      <webApp>
        <url>https://envisionitdev.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>envisionitdev.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
        <siteCollectionAdministrator>pcarson@envisionitdev.onmicrosoft.com</siteCollectionAdministrator>
      </webApp>
    </environment>
    <environment>
      <id>2</id>
      <name>EUM Demo</name>
      <webApp>
        <url>https://eumdemo.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>eumdemo.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
        <siteCollectionAdministrator>pcarson@eumdemo.onmicrosoft.com</siteCollectionAdministrator>
      </webApp>
    </environment>
    <environment>
      <id>3</id>
      <name>Envision IT</name>
      <webApp>
        <url>https://envisionit.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
        <managedCredentials>envisionit.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
        <siteCollectionAdministrator>pcarson@envisionit.com</siteCollectionAdministrator>
      </webApp>
      <graphAPI>
        <managedCredentials>envisionit.onmicrosoft.com AAD</managedCredentials>
        <managedCredentialsType>ClientIdSecret</managedCredentialsType>
        <AADDomain>envisionit.onmicrosoft.com</AADDomain>
      </graphAPI>
    </environment>
    <environment>
      <id>4</id>
      <name>EIT Valo</name>
      <webApp>
        <url>https://eitvalo.sharepoint.com</url>
        <sitesListSiteCollectionPath>/sites/teamwork-modern</sitesListSiteCollectionPath>
        <managedCredentials>eitvalo.sharepoint.com</managedCredentials>
        <managedCredentialsType>UsernamePassword</managedCredentialsType>
        <siteCollectionAdministrator>pcarson@eitvalo.onmicrosoft.com</siteCollectionAdministrator>
      </webApp>
    </environment>
  </environments>
</settings>
