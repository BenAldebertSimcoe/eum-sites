<settings>
    <common>
        <sharePointOnline>true</sharePointOnline>
        <useMixedModeAuthentication>false</useMixedModeAuthentication>
        <siteLists>
            <siteListName>Sites</siteListName>
        </siteLists>
      <defaultEnvironment></defaultEnvironment>
      <managedPaths>
        <path>/sites</path>
        <path>/teams</path>
      </managedPaths>
    </common>
    <environments>
      <environment>
        <id>1</id>
        <name>Envision IT Dev</name>
        <webApp>
          <adminSiteURL>https://envisionitdev-admin.sharepoint.com</adminSiteURL>
          <url>https://envisionitdev.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
          <managedCredentials>envisionitdev.sharepoint.com</managedCredentials>
          <managedCredentialsType>UsernamePassword</managedCredentialsType>
        </webApp>
      </environment>
      <environment>
        <id>2</id>
        <name>Keith Dev</name>
        <webApp>
          <adminSiteURL>https://keithtuomi-admin.sharepoint.com</adminSiteURL>
          <url>https://keithtuomi.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/ENV</sitesListSiteCollectionPath>
          <managedCredentials>keithtuomi.sharepoint.com</managedCredentials>
          <managedCredentialsType>UsernamePassword</managedCredentialsType>
        </webApp>
      </environment>
      <environment>
        <id>3</id>
        <name>Envision IT Dev CLient ID Secret</name>
        <webApp>
          <adminSiteURL>https://envisionitdev-admin.sharepoint.com</adminSiteURL>
          <url>https://envisionitdev.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
          <managedCredentials>envisionitdev.sharepoint.com Client ID</managedCredentials>
          <managedCredentialsType>ClientIdSecret</managedCredentialsType>
        </webApp>
      </environment>
      <environment>
        <id>4</id>
        <name>Envision IT Dev Azure Key Vault</name>
        <webApp>
          <adminSiteURL>https://envisionitdev-admin.sharepoint.com</adminSiteURL>
          <url>https://envisionitdev.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
          <managedCredentials>envisionitdev.sharepoint.com Azure Key Vault</managedCredentials>
          <managedCredentialsType>AzureKeyVault</managedCredentialsType>
          <tenantID>64a54bf1-8d0d-4741-a2ae-36eb0bfa4c58</tenantID>
        </webApp>
      </environment>
    </environments>
</settings>
