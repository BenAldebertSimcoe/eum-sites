<settings>
    <common>
        <sharePointOnline>true</sharePointOnline>
        <useMixedModeAuthentication>false</useMixedModeAuthentication>
        <siteLists>
            <siteListName>Sites</siteListName>
        </siteLists>
      <defaultEnvironment>
      </defaultEnvironment>
      <managedPaths>
        <path>/sites</path>
        <path>/teams</path>
      </managedPaths>
    </common>
    <environments>
      <environment>
        <id>1</id>
        <name>Environment One</name>
        <webApp>
          <url>https://environmentone.sharepoint.com</url>
          <sitesListSiteCollectionPath>/sites/Landing</sitesListSiteCollectionPath>
          <managedCredentials>environmentone.sharepoint.com</managedCredentials>
          <managedCredentialsType>UsernamePassword</managedCredentialsType>
        </webApp>
      </environment>
    </environments>
</settings>
