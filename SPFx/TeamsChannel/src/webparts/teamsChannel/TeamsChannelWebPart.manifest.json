  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart", "TeamsTab"],

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
    "group": { "default": "Other" },
    "title": { "default": "Add channel" },
    "description": { "default": "Request a new teams channel" },
    "officeFabricIconFontName": "TeamsLogo",
    "properties": {
      "WebPartTitle": "{SiteCollectionTitle} Teams Channel Request",
      "MasterSiteUrl": "/sites/landing",
      "SitesListName": "Sites",
      "TeamsChannelsListName": "Teams Channels",
      "ChannelTemplatesListName": "Channel Templates",
      "DivisionsListName": "Divisions"
    }
  }]
}
